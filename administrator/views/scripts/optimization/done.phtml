<script type="text/javascript" charset="utf-8">

    $(document).ready(function(){
        
          var oTable = $('#table_pending').dataTable( {
                "bProcessing": true,
                "bServerSide": true,
                "sAjaxSource": '/administrator/optimization/ajax-done',
                "fnServerParams": function ( aoData ) {  aoData.push( { "name": "filter", "value": 'optimized' } ); },
                "aLengthMenu": [[10, 25, 50, 100, -1], [10, 25, 50, 100, "All"]],
                "iDisplayLength": 100
                ,
                "aoColumns": [
                    { 
                      "sTitle": "PubID",
                      "fnRender": function(obj){ return obj.aData[0]; } 
                    },
                    { 
                      "sTitle": "SiteID",
                      "fnRender": function(obj){ return obj.aData[1]; } 
                    },
                    { 
                      "sTitle": "Website",
                      "fnRender": function(obj){ return '<a href="/administrator/sites/view/id/'+obj.aData[6]+'">'+obj.aData[2]+'</a>'; } 
                    },
                    { 
                      "sTitle": "Contact",
                      "fnRender": function(obj){ return obj.aData[3]; } 
                    },
                    { 
                      "sTitle": "Alexa rank",
                      "fnRender": function(obj){ return obj.aData[4]; } 
                    },
                    { 
                      "sTitle": "Created",
                      "fnRender": function(obj){ var result = '';
                                                 if(obj.aData[5]) result = $.format.date(new Date(obj.aData[5]), "yyyy MMM, dd");
                                                 return result; } 
                    },
                    { 
                      "sTitle": "",
                      "bSortable": false,
                      "fnRender": function(obj){ return '<a href="/administrator/index/auth/id/'+obj.aData[7]+'">login</a>'; } 
                    },
                    { 
                      "sTitle": "",
                      "bSortable": false,
                      "fnRender": function(obj){ return 'Dont Optimizing'; } 
                    },
                    
                ],                                                
                "aaSorting": [[ 1, "asc" ]]

        });
    }); 

</script>

